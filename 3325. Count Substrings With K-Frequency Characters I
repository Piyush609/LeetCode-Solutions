class Solution {
public:
    int numberOfSubstrings(string s, int k) {
        int n = s.size();
        int ans = 0;
        for(int i = 0; i < n; i++){
            vector<int>fr(26,0);
            bool y = false;
            for(int j = i; j < n; j++){
                fr[s[j] - 'a']++;
                
                y = y | (fr[s[j] - 'a'] >= k);
                
                ans += y;
            }
        }
        return ans;
    }
};
