class Solution {
public:
    int countMaxOrSubsets(vector<int>& nums) {
        int n = nums.size();
        int mx = 0;
        int len = 0;
        for(int i = 1; i < (1 << n); i++){
            int x = 0;
            for(int j = 0; j < n; j++){
                if(i & (1 << j)){
                    x = x | nums[j];
                }
            }
            if(mx == x) len++;
            if(mx < x){
                mx = x; len = 1;
            }
        }
        return len;
    }
};
